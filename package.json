{
  "name": "fom",
  "version": "0.0.0",
  "license": "MIT",
  "copyFiles": [
    {
      "from": "apps/servers/gateway/src/templates/*.hbs",
      "to": "dist/apps/servers/gateway/templates/"
    },
    {
      "from": "apps/servers/gateway/src/views/*.hbs",
      "to": "dist/apps/servers/gateway/views/"
    },
    {
      "from": [
        "apps/servers/gateway/src/public/**/*",
        "!apps/servers/gateway/src/public/stylesheets/tailwind.css"
      ],
      "to": "dist/apps/servers/gateway/public/"
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "test-runner": "runner1",
    "copyfiles": "node_modules/.bin/copy-files-from-to",
    "compose:up": "docker-compose up --remove-orphans",
    "dev:gateway": "npm run copyfiles &&  node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve servers-gateway",
    "dev:admin": "node --max_old_space_size=6096 ./node_modules/@angular/cli/bin/ng serve web-admin --hmr --port=5000 -o",
    "dev:school": "node --max_old_space_size=6096 ./node_modules/@angular/cli/bin/ng serve web-school --hmr --port=5100 -o",
    "build:gateway": "node --max_old_space_size=6096 ./node_modules/@angular/cli/bin/ng build servers-gateway --configuration production",
    "build:admin": "set NODE_ENV=production && node --max_old_space_size=6096 ./node_modules/@angular/cli/bin/ng build web-admin --configuration production --stats-json ",
    "build:school": "set NODE_ENV=production && node --max_old_space_size=6096 ./node_modules/@angular/cli/bin/ng build web-school --configuration production --stats-json",
    "bundle-report:admin": "webpack-bundle-analyzer dist/apps/web/admin/stats.json"
  },
  "private": true,
  "dependencies": {
