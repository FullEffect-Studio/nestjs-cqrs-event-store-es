    "web-admin": {
      "projectType": "application",
      "root": "apps/web/admin",
      "sourceRoot": "apps/web/admin/src",
      "prefix": "fom",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/web/admin",
            "index": "apps/web/admin/src/index.html",
            "main": "apps/web/admin/src/main.ts",
            "polyfills": "apps/web/admin/src/polyfills.ts",
            "tsConfig": "apps/web/admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/web/admin/src/favicon.ico",
              "apps/web/admin/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@taiga-ui/icons/src",
                "output": "assets/taiga-ui/icons"
              },
              "apps/web/admin/src/manifest.webmanifest"
            ],
            "styles": [
              "node_modules/@taiga-ui/core/styles/taiga-ui-global.less",
              "node_modules/@taiga-ui/core/styles/taiga-ui-theme.less",
              "apps/web/admin/src/styles.scss",
              "apps/web/admin/src/styles.less"
            ],
            "scripts": [

            ],
            "allowedCommonJsDependencies": [
              "class-validator",
              "validator"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "apps/web/admin/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "15mb",
                  "maximumError": "20mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/web/admin/src/environments/environment.ts",
                  "with": "apps/web/admin/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-admin:build:production"
            },
            "development": {
              "browserTarget": "web-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web/admin/src/**/*.ts",
              "apps/web/admin/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/web/admin"],
          "options": {
            "jestConfig": "apps/web/admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },

