<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Complete Account Setup | Maybert BackOffice</title>
    <script src="https://cdn.jsdelivr.net/npm/vue"></script>
</head>
<body>
<div id="app">
    ${name} {{id}}
</div>
<script>
    console.log('before',Vue.config.delimeters)
    Vue.config.delimeters = ['${', '}']
    console.log('after',Vue.config.delimeters)
    new Vue({
        el: '#app',
        data(){
          return {
            name: 'User Id'
          }
        }
    })
</script>
</body>
</html>

